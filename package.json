  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/cz-commitlint": "^20.3.1",
    "@types/bluebird": "^3.5.42",
    "@types/commander": "^2.12.5",
    "@types/ejs": "^3.1.5",
    "@types/inquirer": "^9.0.9",
